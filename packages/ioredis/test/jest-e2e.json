{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "verbose": true,
  "moduleNameMapper": {
    "@/(.*)": ["<rootDir>/../lib/$1"]
  },
  "setupFilesAfterEnv": ["jest-extended/all"]
}
