{
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "Node16",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ES2021",
    "lib": ["ES2021"],
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false,
    "importHelpers": true,
    "strict": true,
    "skipLibCheck": true
  }
}
